<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Suite">
    <test thread-count="5" name="Regression">
        <!--This will execute all tests in the below package except the excluded ones-->
        <groups>
            <run>
                <exclude name="Smoke"/>
            </run>
        </groups>

        <packages>
            <package name="Loans"/>
        </packages>

    </test> <!-- Test -->
</suite> <!-- Suite -->